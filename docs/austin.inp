# austin.inp
# Austin configuration file

# Adequacy Criterion
tdgCriterion = branch

# Search Method (possible values are: random,hc,chc)
tdgSearchMethod = chc

# Number of decimal places to optimize for hc,chc
hillClimberDecimalPlaces = 2

# Log Keys (mandatory)
logLevel = verbose
logToScreen = true
logCsvResults = true
logTestCases = true 
logToFile = false

# path to AustinLib folder
austinLib = /home/kiran/workspace/austin-sbst/AustinLib/

# Output Base Dir
austinOut = /home/kiran/workspace/Austin/AustinTest/out/


